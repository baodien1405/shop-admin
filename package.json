{
  "name": "shop-admin",
  "private": true,
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && rm -rf src/styles/generated.scss && yarn build:css && vite build",
    "lint": "eslint --ext ts,tsx src/",
    "lint:fix": "eslint --fix --ext ts,tsx src/",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "preview": "vite preview",
    "build:scss": "sass --update src/styles/prime-theme-generator/themes/dc/theme.scss:src/styles/generated.scss --no-source-map"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@react-spring/web": "^10.0.1",
    "@tanstack/react-form": "^1.11.1",
    "@tanstack/react-query": "^5.76.1",
    "axios": "^1.9.0",
    "i18next": "^24.2.0",
    "jwt-decode": "^4.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "react-i18next": "^15.4.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.1.1",
    "rollup-plugin-visualizer": "^5.13.1",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.1",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.5.3",
    "sass": "^1.83.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
